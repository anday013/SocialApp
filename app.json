{
  "expo": {
    "name": "Looks",
    "slug": "Looks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2B85F5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anday2000.Looks"
    },
    "web": {
      "favicon": "./assets/splash.png"
    },
    "android": {
      "package": "com.anday2000.Looks",
      "useNextNotificationsApi": true
    }
  }
}

{
    "expo": {
        "name": "Looks",
        "slug": "Looks",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#2B85F5"
        },
        "updates": {
